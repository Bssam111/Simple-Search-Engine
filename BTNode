public class BSTNode<T> {  
    public int key;  
    public T data;  
    public BSTNode<T> left,right;  
  
    //node without left and right  
    public BSTNode(int k,T val) {  
        key=k;  
        data=val;  
        left=right=null;  
    }  
  
    //node with left right  
  
    public BSTNode(T val, BSTNode<T> l, BSTNode<T> r) {  
        key=k;  
        data=val;  
        left=l;  
        right=r;  
    }  
}  
